@prefix : <http://www.semanticweb.org/santo/ontologies/2025/0/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix snomed: <http://purl.bioontology.org/ontology/SNOMEDCT> .
@prefix apaonto: <http://ontology.apa.org/apaonto#> .
@base <http://www.semanticweb.org/santo/ontologies/2025/0/> .

<http://www.semanticweb.org/santo/ontologies/2025/0> rdf:type owl:Ontology ;
                                                      owl:imports <http://example.org/myontology> ;
                                                      <http://purl.org/dc/elements/1.1/creator> "Gabriele Santoro"@it ;
                                                      <http://purl.org/dc/elements/1.1/date> "2024-01-01T00:00:00"^^xsd:dateTime ;
                                                      <http://purl.org/dc/elements/1.1/description> "La seguente ontologia, sviluppata per il progetto di Modellazione Concettuale per il Web Semantico si basa sul dominio della psicologia/psichiatria, cercando di rappresentare le principali classi di disturbo, di terapia e di professionista sanitario e mettendo in correlazione queste tre macro entità con lo scopo di consigliare ad un ipotetico paziente la giusta terapia e il conseguente contatto sanitario più adatto."@it ;
                                                      <http://purl.org/dc/elements/1.1/title> "Terapie" ;
                                                      vann:preferredNamespacePrefix "terapie:" ;
                                                      vann:preferredNamespaceUri "http://www.semanticweb.org/santo/ontologies/Terapie" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/santo/ontologies/2025/0/collegatoA
:collegatoA rdf:type owl:ObjectProperty ;
            rdfs:domain :Disturbo ;
            rdfs:range :Disturbo ;
            rdfs:label "collegatoA"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/efficacePer
:efficacePer rdf:type owl:ObjectProperty ;
             owl:inverseOf :èEfficaceUna ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain :Terapia ;
             rdfs:range :Disturbo ;
             rdfs:label "efficacePer"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/faUsoDi
:faUsoDi rdf:type owl:ObjectProperty ;
         owl:inverseOf :èUsatoIn ;
         rdf:type owl:InverseFunctionalProperty ;
         rdfs:domain :TerapiaFarmacologica ;
         rdfs:range :Medicinale ;
         rdfs:label "faUsoDi"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/haEffetto
:haEffetto rdf:type owl:ObjectProperty ;
           rdfs:domain :Medicinale ;
           rdfs:range :Effetto ;
           rdfs:label "haEffetto"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/haGravità
:haGravità rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Disturbo ;
           rdfs:range :Gravità ;
           rdfs:label "haGravità"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/haInCura
:haInCura rdf:type owl:ObjectProperty ;
          rdfs:domain :ProfessionistaSanitario ;
          rdfs:range :Paziente ;
          rdfs:label "haInCura"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/metodologiaUtilizzataDa
:metodologiaUtilizzataDa rdf:type owl:ObjectProperty ;
                         owl:inverseOf :utilizzaMetodologia ;
                         rdf:type owl:InverseFunctionalProperty ;
                         rdfs:domain :Metodologia ;
                         rdfs:range :Terapia ;
                         vann:preferredNamespacePrefix "metodologiaUtilizzataDa"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/prescrittoPer
:prescrittoPer rdf:type owl:ObjectProperty ;
               rdfs:domain :Medicinale ;
               rdfs:range :Disturbo ;
               rdfs:label "prescrittoPer"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/prescrive
:prescrive rdf:type owl:ObjectProperty ;
           rdfs:domain :Psichiatra ;
           rdfs:range :Medicinale ;
           rdfs:label "prescrive"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/puòEssereTrattatoDa
:puòEssereTrattatoDa rdf:type owl:ObjectProperty ;
                     owl:inverseOf :puòTrattareUn ;
                     rdfs:domain :Disturbo ;
                     rdfs:range :ProfessionistaSanitario ;
                     rdfs:label "puòEssereTrattatoDa"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/puòTrattareUn
:puòTrattareUn rdf:type owl:ObjectProperty ;
               rdfs:domain :ProfessionistaSanitario ;
               rdfs:range :Disturbo ;
               rdfs:label "puòTrattareUn"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/soffreDi
:soffreDi rdf:type owl:ObjectProperty ;
          rdfs:domain :Paziente ;
          rdfs:range :Disturbo ;
          rdfs:label "soffreDi"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/svolge
:svolge rdf:type owl:ObjectProperty ;
        owl:inverseOf :èSvoltaDa ;
        rdf:type owl:InverseFunctionalProperty ;
        rdfs:domain :ProfessionistaSanitario ;
        rdfs:range :Terapia ;
        rdfs:label "svolge"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/utilizzaMetodologia
:utilizzaMetodologia rdf:type owl:ObjectProperty ,
                              owl:InverseFunctionalProperty ;
                     rdfs:domain :Terapia ;
                     rdfs:range :Metodologia ;
                     rdfs:label "utilizzaMetodologia"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/èCorrelatoA
:èCorrelatoA rdf:type owl:ObjectProperty ;
             rdfs:domain :Disturbo ;
             rdfs:range :Correlazione ;
             rdfs:label "èCorrelatoA"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/èEfficaceUna
:èEfficaceUna rdf:type owl:ObjectProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain :Disturbo ;
              rdfs:range :Terapia ;
              rdfs:label "èEfficaceUna"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/èInCuraDa
:èInCuraDa rdf:type owl:ObjectProperty ;
           rdfs:domain :Paziente ;
           rdfs:range :ProfessionistaSanitario ;
           rdfs:label "èInCuraDa"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/èSoffertoDa
:èSoffertoDa rdf:type owl:ObjectProperty ;
             rdfs:domain :Disturbo ;
             rdfs:range :Paziente ;
             rdfs:label "èSoffertoDa"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/èSvoltaDa
:èSvoltaDa rdf:type owl:ObjectProperty ,
                    owl:InverseFunctionalProperty ;
           rdfs:domain :Terapia ;
           rdfs:range :ProfessionistaSanitario ;
           rdfs:label "èSvoltaDa"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/èUsatoIn
:èUsatoIn rdf:type owl:ObjectProperty ,
                   owl:InverseFunctionalProperty ;
          rdfs:domain :Medicinale ;
          rdfs:range :TerapiaFarmacologica ;
          rdfs:label "èUsatoIn"@it .


#################################################################
#    Classes
#################################################################

###  http://ontology.apa.org/apaonto/Drugs
<http://ontology.apa.org/apaonto/Drugs> rdf:type owl:Class ;
                                        owl:equivalentClass :Medicinale ;
                                        rdfs:comment "La classe Drug è allineata dall'ontologia Apaonto, più nel dettaglio alla classe Drugs. Apaonto è un'ontologia correlata al mondo della medicina psicologica e psichiatrica."@it ;
                                        rdfs:label "Drug"@en .


###  http://purl.bioontology.org/ontology/SNOMEDCT/74732009
<http://purl.bioontology.org/ontology/SNOMEDCT/74732009> rdf:type owl:Class ;
                                                         owl:equivalentClass :Disturbo ;
                                                         rdfs:comment "La classe Mental Disorder è allineata alla classe Mental Disorder dell'ontologia Snomed, un'altra grande ontologia riferita al mondo psicologico e psichiatrico. In questo caso l'allineamento è stato fatto per mettere in correlazione il concetto di Disturbo con un'accezione sia psicologica che psichiatrica"@it ;
                                                         rdfs:label "MentalDisorder"@en .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Ansia
:Ansia rdf:type owl:Class ;
       rdfs:subClassOf :Correlazione .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Ansiolitici
:Ansiolitici rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :haEffetto ;
                                   owl:someValuesFrom :Sedativo
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :prescrittoPer ;
                                   owl:someValuesFrom :DisturboDiAnsia
                                 ] ;
             rdfs:subClassOf :Medicinale ;
             rdfs:comment "La classe Ansiolitco modella il concetto di medicinale ansiolitico. Questi farmaci sono usati per trattare l'ansia e i disturbi correlati. Aiutano a ridurre la tensione e promuovono il rilassamento"@it ;
             rdfs:label "Ansiolitici"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Antidepressivi
:Antidepressivi rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :haEffetto ;
                                      owl:someValuesFrom :StabilizzatoreDelloUmore
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :prescrittoPer ;
                                      owl:someValuesFrom :DisturboDelloUmore
                                    ] ;
                rdfs:subClassOf :Medicinale ;
                rdfs:comment "La classe Antidepressivi modella il concetto di medicinale antidpressivo. Questi farmaci sono utilizzati per trattare la depressione e altri disturbi dell'umore. Aiutano a migliorare l'umore e ridurre i sintomi depressivi"@it ;
                rdfs:label "Antidepressivi"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Antipsicotici
:Antipsicotici rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :haEffetto ;
                                     owl:someValuesFrom :Neurolettico
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :prescrittoPer ;
                                     owl:someValuesFrom :DisturboPsicotico
                                   ] ;
               rdfs:subClassOf :Medicinale ;
               rdfs:comment "La classe Antipsicotici modella il concetto di medicinale antipsicotico. Questi farmaci sono prescritti per trattare disturbi psicotici come la schizofrenia e il disturbo bipolare. Aiutano a ridurre i sintomi psicotici come allucinazioni e deliri"@it ;
               rdfs:label "Antipsicotici"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Corporea
:Corporea rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :metodologiaUtilizzataDa ;
                                owl:someValuesFrom :TerapiaCorporea
                              ] ;
          rdfs:subClassOf :Metodologia ;
          rdfs:comment "La classe Corporea modella il concetto di metodologia corporea, una tipo di psicoterapia che usata tecniche fisiche di vario tipo per gestire ad esempio disturbi di Ansia e PTSD."@it ;
          rdfs:label "Corporea"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Correlazione
:Correlazione rdf:type owl:Class ;
              rdfs:comment "La classe correlazione serve a definire, in maniera rapida e diretta, la sfera a cui è associata una classe di disturbi"@it ;
              rdfs:label "Correlazione"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Disturbo
:Disturbo rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :collegatoA ;
                                owl:someValuesFrom :Disturbo
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :haGravità ;
                                owl:someValuesFrom :Gravità
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :puòEssereTrattatoDa ;
                                owl:someValuesFrom :ProfessionistaSanitario
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :èCorrelatoA ;
                                owl:someValuesFrom :Correlazione
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :èEfficaceUna ;
                                owl:someValuesFrom :Terapia
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :èSoffertoDa ;
                                owl:someValuesFrom :Paziente
                              ] ;
          owl:disjointUnionOf ( :DisturboDellaPersonalità
                                :DisturboDelloUmore
                                :DisturboDiAnsia
                                :DisturboPsicosomatico
                                :DisturboPsicotico
                              ) ;
          rdfs:comment "La classe Disturbo serve a definire il concetto generale di disturbo psicologico/psichiatrico in maniera generica"@it ;
          rdfs:label "Disturbo"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/DisturboDellaPersonalità
:DisturboDellaPersonalità rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty :èCorrelatoA ;
                                                owl:someValuesFrom :Personalità
                                              ] ;
                          rdfs:subClassOf :Disturbo ;
                          rdfs:comment "La classe DisturboDellaPersonalità modella il concetto di disturbo psicologico/psichiatrico correlato alla sfera della personalità di un individuo"@it ;
                          rdfs:label "DisturboDiPersonalità"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/DisturboDelloUmore
:DisturboDelloUmore rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :èCorrelatoA ;
                                          owl:someValuesFrom :Umore
                                        ] ;
                    rdfs:subClassOf :Disturbo ;
                    rdfs:comment "La classe DisturboDelloUmore modella il concetto di disturbo psicologico/psichiatrico correlato alla sfera dell'umore di un individuo"@it ;
                    rdfs:label "DisturboDelloUmore"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/DisturboDiAnsia
:DisturboDiAnsia rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :èCorrelatoA ;
                                       owl:someValuesFrom :Ansia
                                     ] ;
                 rdfs:subClassOf :Disturbo ;
                 rdfs:comment "La classe DisturboDiAnsia modella il concetto di disturbo psicologico/psichiatrico correlato alla sfera dell'ansia di un individuo"@it ;
                 rdfs:label "DisturboDiAnsia"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/DisturboPsicosomatico
:DisturboPsicosomatico rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty :èCorrelatoA ;
                                             owl:someValuesFrom :Psicosomatico
                                           ] ;
                       rdfs:subClassOf :Disturbo ;
                       rdfs:comment "La classe DisturboPsicosomatico modella il concetto di disturbo psicologico/psichiatrico correlato alla sfera dei dei sintomi psicosamatici di un individuo conseguenti ad un qualche problema psicologico/psichiatrico"@it ;
                       rdfs:label "DisturboPsicosomatico"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/DisturboPsicotico
:DisturboPsicotico rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :èCorrelatoA ;
                                         owl:someValuesFrom :Psicotico
                                       ] ;
                   rdfs:subClassOf :Disturbo ;
                   rdfs:comment "La classe DisturboPsicotico modella il concetto di disturbo psicologico/psichiatrico correlato alla sfera della psicosi di un individuo, concettualmente è un tipo disturbo naturalmente psicotico"@it ;
                   rdfs:label "DisturboPsicotico"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Effetto
:Effetto rdf:type owl:Class ;
         rdfs:comment "La classe Effetto serve a definire in maniera astratta alcuni dei possibile effetti di classi di medicinali utilizzati in campo psichiatrico. Nella seguente ontologia sono stati presi solo pochi principi psicoattivi delle prinicipali classi di medicinali esistenti."@it ;
         rdfs:label "Effetto"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Farmacologica
:Farmacologica rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :metodologiaUtilizzataDa ;
                                     owl:someValuesFrom :TerapiaFarmacologica
                                   ] ;
               rdfs:subClassOf :Metodologia ;
               rdfs:comment "La classe Farmacologica modella il concetto di metodologia farmacologica, quindi è riferita alla terapia psichiatrica che fa uso di medicinali con effetti di vario tipo per gestire disturbi"@it ;
               rdfs:label "Farmacologica"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Gravità
:Gravità rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Grave
                                           :Lieve
                                         )
                             ] ;
         rdfs:comment "La classe Gravità è una classe enumerata che serve ad associare un qualche grado di gravità ad un disturbo di un paziente. In questo caso la gravità si riferisce alla percezione che il paziente ha dei suoi sintomi piuttosto che ad una scalta indicativa che un professionista sanitario può assegnare dopo una serie di colloqui."@it ;
         rdfs:label "Gravità"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Medicinale
:Medicinale rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :haEffetto ;
                                  owl:someValuesFrom :Effetto
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :prescrittoPer ;
                                  owl:someValuesFrom :Disturbo
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :èUsatoIn ;
                                  owl:someValuesFrom :TerapiaFarmacologica
                                ] ;
            rdfs:comment "La classe Medicinale modella il concetto generale di farmaco psichiatrico ed è definita equivalentTo alla classe Drugs dell'ontologia esterna Apaonto."@it ;
            rdfs:label "Medicinale"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Metodologia
:Metodologia rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :metodologiaUtilizzataDa ;
                                   owl:someValuesFrom :Terapia
                                 ] ;
             rdfs:comment "La classe Metodologia modella il concetto di metodologia di una terapia, è stata inserita per differenziare i tipi di terapia sia svolti da psicoterapeuti che da psichiatri. Nell'ontologia abbiamo tre principali metodologie, verbale, corporea e farmacologica"@it ;
             rdfs:label "Metodologia"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Neurolettico
:Neurolettico rdf:type owl:Class ;
              rdfs:subClassOf :Effetto ;
              rdfs:comment "La classe Neurolettico modella il concetto di effetto Neurolettico, Gli effetti neurolettici dei medicinali psichiatrici si riferiscono agli effetti che questi farmaci hanno sul sistema nervoso centrale, in particolare attraverso il blocco dei recettori della dopamina."@it ;
              rdfs:label "Neurolettico"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Paziente
:Paziente rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :soffreDi ;
                                owl:someValuesFrom :Disturbo
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :èInCuraDa ;
                                owl:someValuesFrom :ProfessionistaSanitario
                              ] ;
          rdfs:comment "La classe Paziente serve a definire il concetto generale di paziente. In questo caso l'accezione di paziente è riferita sia alla persona che svolge un qualche tipo di terapia ma anche a colui che vuole iniziarla."@it ;
          rdfs:label "Paziente"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Personalità
:Personalità rdf:type owl:Class ;
             rdfs:subClassOf :Correlazione ;
             rdfs:comment "La classe Personalità modella il concetto di personalità nella sua accezione più astratta e generica"@it ;
             rdfs:label "Personalità"@en .


###  http://www.semanticweb.org/santo/ontologies/2025/0/ProfessionistaSanitario
:ProfessionistaSanitario rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Restriction ;
                                               owl:onProperty :haInCura ;
                                               owl:someValuesFrom :Paziente
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :puòTrattareUn ;
                                               owl:someValuesFrom :Disturbo
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :svolge ;
                                               owl:someValuesFrom :Terapia
                                             ] ;
                         rdfs:comment "La classe ProfessionsitaSanitario modella il concetto di figura medica che lavoro nel campo psicologico e psichiatrico."@it ;
                         rdfs:label "ProfessionistaSanitario"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Psichiatra
:Psichiatra rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :prescrive ;
                                  owl:someValuesFrom :Medicinale
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :svolge ;
                                  owl:someValuesFrom :TerapiaFarmacologica
                                ] ;
            rdfs:subClassOf :ProfessionistaSanitario ;
            rdfs:comment "La classe Psichiatra modella il concetto di figura medica specializzata in terapia psichiatrica"@it ;
            rdfs:label "Psichiatra"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Psicosomatico
:Psicosomatico rdf:type owl:Class ;
               rdfs:subClassOf :Correlazione ;
               rdfs:comment "La classe Psicosomatico modella il concetto di problema psicosomatico nella sua accezione più astratta e generica."@it ;
               rdfs:label "Psicosomatico"@en .


###  http://www.semanticweb.org/santo/ontologies/2025/0/PsicoterapeutaCorporeo
:PsicoterapeutaCorporeo rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :svolge ;
                                              owl:someValuesFrom :TerapiaCorporea
                                            ] ;
                        rdfs:subClassOf :ProfessionistaSanitario ;
                        rdfs:comment "La classe PsicoterapeutaCorporea modella il concetto di figura medica psicoterapeutica specializzata in varie tecniche che coinvolgono azioni maggiormente fisiche nel paziente, quindi che usa una metodologia corporea"@it ;
                        rdfs:label "PsicoterapeutaCorporeo"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/PsicoterapeutaVerbale
:PsicoterapeutaVerbale rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty :svolge ;
                                             owl:someValuesFrom :Psicoterapia
                                           ] ;
                       rdfs:subClassOf :ProfessionistaSanitario ;
                       rdfs:comment "La classe PsicoterapeutaVerbale modella il concetto di figura medica psicoterapeutica che fa uso di metodologie verbali"@it ;
                       rdfs:label "PsicoterapeutaVerbale"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Psicoterapia
:Psicoterapia rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :utilizzaMetodologia ;
                                    owl:someValuesFrom :Verbale
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :èSvoltaDa ;
                                    owl:someValuesFrom :PsicoterapeutaVerbale
                                  ] ;
              rdfs:subClassOf :Terapia ;
              rdfs:comment "La classe Psicoterapia serve a definire il concetto generico di psicoterapia, ovvero il tipo di terapia svolta da professionisti che hanno svolto un qualche tipo di scuola specializzante di psicoterapia"@it ;
              rdfs:label "Psicoterapia"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Psicotico
:Psicotico rdf:type owl:Class ;
           rdfs:subClassOf :Correlazione ;
           rdfs:comment "La classe Psicotico modella il concetto di psicotico nella sua accezione più astratta e generica"@it ;
           rdfs:label "Psicotico"@en .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Sedativo
:Sedativo rdf:type owl:Class ;
          rdfs:subClassOf :Effetto ;
          rdfs:comment "La classe Sedativo modella il concetto di sedativo.Gli effetti sedativi si riferiscono alla capacità di alcuni farmaci di indurre uno stato di calma, rilassamento o sonnolenza"@it ;
          rdfs:label "Sedativo"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/StabilizzatoreDelloUmore
:StabilizzatoreDelloUmore rdf:type owl:Class ;
                          rdfs:subClassOf :Effetto ;
                          rdfs:comment "La classe StabilizzatoreDelloUmore modella il concetto di stabilizzatore dell'umore. Gli stabilizzatori dell'umore sono una categoria di farmaci utilizzati principalmente per trattare il disturbo bipolare, una condizione caratterizzata da oscillazioni estreme dell'umore tra episodi di mania e depressione."@it ;
                          rdfs:label "StabilizzatoreDelloUmore"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Terapia
:Terapia rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :efficacePer ;
                               owl:someValuesFrom :Disturbo
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :utilizzaMetodologia ;
                               owl:someValuesFrom :Metodologia
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :èSvoltaDa ;
                               owl:someValuesFrom :ProfessionistaSanitario
                             ] ;
         rdfs:comment "La classe Terapia serve a definire il concetto di terapia psicologica/psichiatrica che viene svolta da un qualche professionista sanitario e da un paziente con un qualche disturbo"@it ;
         rdfs:label "Terapia"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/TerapiaCognitivoComportamentale
:TerapiaCognitivoComportamentale rdf:type owl:Class ;
                                 rdfs:subClassOf :Psicoterapia ;
                                 rdfs:comment "La classe TerapiaCognitivoComportamentale è un tipo di terapia svolta da psicoterapeutici che hanno svolto una specifica scuola che si basa sul modello Cognitivo comportamentale. Questo modello si concentra su come i pensieri (cognizioni) e i comportamenti influenzano le emozioni e le azioni"@it ;
                                 rdfs:label "TerapiaCognitivoComportamentale"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/TerapiaCorporea
:TerapiaCorporea rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :utilizzaMetodologia ;
                                       owl:someValuesFrom :Corporea
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :èSvoltaDa ;
                                       owl:someValuesFrom :PsicoterapeutaCorporeo
                                     ] ;
                 rdfs:subClassOf :Terapia ;
                 rdfs:comment "La classe TerapiaCorporea si riferisce ad una terapia svolta da uno psicoterapeuta specializzata in metodologia corporea"@it ;
                 rdfs:label "TerapiaCorporea"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/TerapiaFarmacologica
:TerapiaFarmacologica rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty :faUsoDi ;
                                            owl:someValuesFrom :Medicinale
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :utilizzaMetodologia ;
                                            owl:someValuesFrom :Farmacologica
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :èSvoltaDa ;
                                            owl:someValuesFrom :Psichiatra
                                          ] ;
                      rdfs:subClassOf :Terapia ;
                      rdfs:comment "La classe TerapiaFarmacologica modella il concetto di terapia farmacologica, ovvero la terapia psichiatrica svolta da uno professionista psichiatrico e che quindi fa uso di medicinali per trattare disturbi di vario tipo"@it ;
                      rdfs:label "TerapiaFarmacologica"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/TerapiaPsicodinamica
:TerapiaPsicodinamica rdf:type owl:Class ;
                      rdfs:subClassOf :Psicoterapia ;
                      rdfs:comment "La classe TerapiaPsicodinamica è un tipo di terapia svolta da psicoterapeutici che hanno svolto una specifica scuola che si basa sul modello Psicodinamico. Questo modello si basa sulle teorie di Sigmund Freud, ed esplora l'influenza dell'inconscio e delle esperienze passate sul comportamento attuale"@it ;
                      rdfs:label "TerapiaPsicodinamica"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/TerapiaSistemicoRelazionale
:TerapiaSistemicoRelazionale rdf:type owl:Class ;
                             rdfs:subClassOf :Psicoterapia ;
                             rdfs:comment "La classe TerapiaSistemicoRelazionale è un tipo di terapia svolta da psicoterapeutici che hanno svolto una specifica scuola che si basa sul modello Sistemico-relazionale. Questo modello considera gli individui all'interno dei loro sistemi relazionali, come la famiglia o la comunità"@it ;
                             rdfs:label "TerapiaSistemicoRelazionale"@it .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Umore
:Umore rdf:type owl:Class ;
       rdfs:subClassOf :Correlazione ;
       rdfs:comment "La classe Umore modella il concetto di umore nella sua accezione più astratta e generica"@it ;
       rdfs:label "Umore"@en .


###  http://www.semanticweb.org/santo/ontologies/2025/0/Verbale
:Verbale rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :metodologiaUtilizzataDa ;
                               owl:someValuesFrom :Psicoterapia
                             ] ;
         rdfs:subClassOf :Metodologia ;
         rdfs:comment "La classe Verbale modella il concetto di metodologia verbale, ovvero la generica psicoterapia che comporta colloqui e analisi dell'individuol dei suoi comportamenti e delle sue esperienze."@it ;
         rdfs:label "Verbale"@it .


